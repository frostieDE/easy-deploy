<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style TargetType="TextBlock" x:Key="ListBoxDeployActionTextBlock">
        <Setter Property="FontWeight" Value="SemiBold" />
        <Setter Property="FontSize" Value="12" />
    </Style>

    <Style TargetType="TextBlock" x:Key="ListBoxDeployActionDescriptionTextBlock">
        <Setter Property="FontSize" Value="12" />
        <Setter Property="Margin" Value="0,3,0,0" />
    </Style>

    <Style TargetType="TextBlock" x:Key="TextBoxLabelTextBlock">
        <Setter Property="FontSize" Value="12" />
    </Style>

    <Style TargetType="TextBox" x:Key="DefaultTextBox">
        <Setter Property="Padding" Value="3" />
        <Setter Property="FontSize" Value="12" />
        <Setter Property="Margin" Value="0,5" />
    </Style>

    <Style TargetType="Button" x:Key="DefaultButton">
        <Setter Property="Padding" Value="3" />
        <Setter Property="FontSize" Value="12" />
        <Setter Property="Margin" Value="5" />
    </Style>
</ResourceDictionary>